{
	"title": "Custom dialogs",
	"window_size": [
		300,
		600
	],
	"body": [
		{
			"label": {
				"text": "Long text to show how label works"
			}
		},
		{
			"text": {
				"id": "name",
				"label": "name",
				"required": true,
				"placeholder": "your name"
			}
		},
		{
			"text": {
				"id": "description",
				"multiline": true,
				"label": "description",
				"placeholder": "description"
			}
		},
		{
			"calendar": {
				"id": "date",
				"label": "date",
				"date": "2016-02-01"
			}
		},
		{
			"check": {
				"id": "like_check",
				"required": true,
				"label": "Do you like it?"
			}
		},
		{
			"color": {
				"id": "color",
				"required": true,
				"label": "Select color"
			}
		},
		{
			"combobox": {
				"id": "select",
				"label": "Select one",
				"required": true,
				"options": [
					"0. bat",
					"1. dos",
					"2. trois",
					"3. bat",
					"4. dos",
					"5. trois",
					"6. bat",
					"7. dos",
					"8. trois",
					"9. bat",
					"10. dos"
				]
			}
		},
		{
			"list": {
				"id": "list",
				"required": true,
				"header": "Header",
				"values": [
					"0. bat",
					"1. dos",
					"2. trois",
					"3. bat",
					"4. dos",
					"5. trois",
					"6. bat",
					"7. dos",
					"8. trois",
					"9. bat",
					"10. dos",
					"11. trois",
					"12. bat",
					"13. dos",
					"14. trois",
					"15. bat",
					"16. dos",
					"17. trois",
					"18. bat",
					"19. dos",
					"20. trois"
				]
			}
		},
		{
			"radio": {
				"id": "like_radio",
				"label": "Do you like it?",
				"selected": "50/50",
				"options": [
					"nope",
					"50/50",
					"amazing!"
				]
			}
		},
		{
			"progress": {
				"id": "progress",
				"label": "Progress"
			}
		},
		{
			"link": {
				"id": "progress",
				"label": "Clialogs github",
				"url": "http://github.com/ideckia/clialogs"
			}
		}
	]
}